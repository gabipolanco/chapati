<h2>Edit place {{name}}</h2>

<form action="/private/place/{{_id}}/edit" method="POST" enctype="multipart/form-data">
  <input type="text" name="user" value="{{ownerId}}" hidden>
  <label for="name">Name:
    <input type="text" name="name" id="name" value="{{name}}">
  </label>
  <label for="image">Image:
    <input type="file" name="image" id="image">
  </label>
  <input type="submit" value="Edit place">
</form>